{
  "name": "directapp-module-schema-builder",
  "version": "1.0.0",
  "type": "module",
  "description": "Visual schema builder for Directus with field access control, relations, and grouping",
  "directus:extension": {
    "type": "module",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^11.12.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^16.0.2",
    "typescript": "^5.3.3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
